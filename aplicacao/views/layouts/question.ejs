				<form class="col-11 col-md-8" id="question-form" action="" method="POST">
					<div class="row v-divider"></div>
					<div class="row">
						<div class="col centering question">
							<%= title %>
						</div>
					</div>
					<div class="row v-divider"></div>
					<%- include( `../${question}`  ) %>
					<div class="row v-divider"></div>
					<div class="row justify-content-between">
						<div class="col-5 col-md-4 col-lg-3"><a href="javascript:history.back()" class="btn btn-hw-sm color-medium btn-block" role="button">Anterior</a></div>
						<div class="col-5 col-md-4 col-lg-3">
							<input type="submit" value="Próximo" class="btn btn-hw-sm color-medium btn-block disabled" role="button" aria-disabled="true" id="next-button">
							<!-- <a href="<%= next %>"  class="btn btn-hw-sm color-medium btn-block disabled" role="button" aria-disabled="true" id="next-button">Próximo</a> -->
						</div>
					</div>
					<div class="row v-divider"></div>
					<div class="row">
						<div class="col-12">
							<div class="progress">
								<div class="progress-bar progress-bar-striped progress-bar-animated color-heavy" role="progressbar" aria-valuenow="<%= percent %>" aria-valuemin="0" aria-valuemax="100" style="width:<%= percent %>%"><%= percent %>%</div>
							</div>
						</div>
					</div>
					<% if (footnote) { %>
<%- include( `../${footnote}` ) %>
					<% } %>
				</form>